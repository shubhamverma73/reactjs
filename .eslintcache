ser} />\r\n                  <Route exact path=\"/users/view/:id\" component={ViewUser} />\r\n                  <Route exact path=\"/users/edit/:id\" component={EditUser} />\r\n                  <Route exact path=\"/hookcall\" render={() =>\r\n                    <>\r\n                      <Hookcall button=\"Click Here\" />\r\n                      <Runtime />\r\n                    </>\r\n                  } />\r\n                  <Route exact path=\"/increment-value\" component={IncreaseCount} />\r\n                  <Route exact path=\"/call-js-file\" component={JsFile} />\r\n                  <Route exact path=\"/context\" component={Compa} />\r\n                  <Route exact path=\"/card-app\" component={Cardapp} />\r\n                  <Route exact path=\"/image-upload\" component={Imageupload} />\r\n                  <Route exact path=\"/slider\" component={Slider} />\r\n                  <Route exact path=\"/dropdown\" component={Dropdown} />\r\n                  <Route exact path=\"/pass-value-to-function\" component={Passvalue} />\r\n                  <Route exact path=\"/multiple-fields\" component={Multiplefields} />\r\n                  <Route exact path=\"/datepicker\" component={Datepicker} />\r\n                  <Route exact path=\"/dropdownnew\" component={Dropdownnew} />\r\n                  {/* <Route component={NotFound} /> */}\r\n                  <Redirect to='/' />\r\n                </Switch>\r\n              </div>\r\n            </Router>\r\n            \r\n          </>\r\n        );\r\n}\r\n\r\nexport default App;\r\n","D:\\xampp\\htdocs\\test_files\\react\\react\\src\\temp\\Cardapp.js",[],"D:\\xampp\\htdocs\\test_files\\react\\react\\src\\temp\\JsFile.js",[],"D:\\xampp\\htdocs\\test_files\\react\\react\\src\\temp\\IncrementVal.jsx",[],"D:\\xampp\\htdocs\\test_files\\react\\react\\src\\temp\\Hookcall.jsx",["122"],"import React, { useState } from 'react';\r\nimport { propTypes } from 'react-bootstrap/esm/Image';\r\n\r\n\r\nconst Hookcall = (props) => {\r\n    let currTIme = new Date().toLocaleTimeString();\r\n\r\n    const [currentTIme, setTIme] = useState(currTIme); //useState is hook function that return current data and function\r\n\r\n    const ChangeTIme = () => {\r\n        currTIme = new Date().toLocaleTimeString();\r\n        setTIme(currTIme);\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <div className=\"manageDivCountBoxTime\">\r\n                <span id=\"timeCount\">{currentTIme}</span><br/><br/>\r\n                <button className=\"buttonClass\" onClick={ChangeTIme}>{props.button}</button>\r\n            </div>\r\n        </>\r\n    );\r\n}\r\n\r\nconst Runtime = () => {\r\n    let currTIme = new Date().toLocaleTimeString();\r\n\r\n    const [currentTIme, setTime] = useState(currTIme);\r\n\r\n    const updateTIme = () => {\r\n        currTIme = new Date().toLocaleTimeString();\r\n        setTime(currTIme);\r\n    };\r\n    setInterval(updateTIme, 1000);\r\n\r\n    return (\r\n        <>\r\n            <div className=\"manageDivAutoTime\">\r\n                <span id=\"timeAutoCount\">{currentTIme}</span>\r\n            </div>\r\n        </>\r\n    );\r\n}\r\n\r\nexport default Hookcall;\r\nexport { Runtime };","D:\\xampp\\htdocs\\test_files\\react\\react\\src\\temp\\Passvalue.jsx",[],"D:\\xampp\\htdocs\\test_files\\react\\react\\src\\context\\Compa.jsx",[],"D:\\xampp\\htdocs\\test_files\\react\\react\\src\\temp\\Dropdown.jsx",[],"D:\\xampp\\htdocs\\test_files\\react\\react\\src\\temp\\Multiplefields.jsx",["123"],"import React, { Fragment, useEffect, useState } from 'react'\r\nimport { useForm, useFieldArray } from 'react-hook-form';\r\nimport Select from 'react-select';\r\nimport { stateOptions } from '../assets/data_new';\r\n\r\nconst UserInformation = (props) => {\r\n    const { register, control } = props;\r\n    const { append, fields, remove } = useFieldArray({\r\n        control,\r\n        name: \"users\"\r\n    });\r\n    const onClickAdd = () => {\r\n        